# Base URL for the Campaign AI backend API (no trailing slash)
VITE_API_BASE_URL=https://api.example.com

# OAuth client IDs for providers supported by the auth service
VITE_OAUTH_GOOGLE_CLIENT_ID=your-google-client-id

# Server-side Google OAuth configuration consumed by the Supabase Edge Function handlers
# (e.g., src/supabase/functions/make-server/index.ts and src/supabase/functions/server/index.tsx)
GOOGLE_OAUTH_CLIENT_ID=your-google-client-id
GOOGLE_OAUTH_REDIRECT_URI=https://your-app.example.com/api/auth/callback/google
